#include "Unit1.h"
#include "Unit2.h"
#include "Unit3.h"
#include "Unit4.h"
#include "Unit5.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)
#pragma resource "*.dfm"
TForm5 *Form5;
//---------------------------------------------------------------------------
__fastcall TForm5::TForm5(TComponent* Owner)
	: TForm(Owner)
{
}
//---------------------------------------------------------------------------
void __fastcall TForm5::Button1Click(TObject *Sender)
{
	int rowsCounter;
	for (int i = 1; i < StrToInt(Form1->StringGrid1->RowCount); i++)
	{
		for (int j = 0; j < StrToInt(Form1->StringGrid1->ColCount); j++)
		{
			if(StrToInt(Edit1->Tetx) == StrToInt(Form1->StringGrid1->Cells[3][i]))
			{
				Form3->StringGrid1->Cells[0][1] = Form1->StringGrid1->Cells[0][1];
			}
		}
	}
	Form3->Show();
}
//---------------------------------------------------------------------------

